{
  "name": "palette-sdk-typescript",
  "version": "4.6.41",
  "description": "TypeScript SDK for Spectro Cloud Palette API",
  "main": "generated/index.ts",
  "types": "generated/index.ts",
  "files": [
    "generated/**/*",
    "test/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
