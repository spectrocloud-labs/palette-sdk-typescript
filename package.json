    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "openapitools": "github:openapitools/openapi-generator-cli",
    "orval": "^7.10.0",
    "prettier": "3.6.2",
    "typescript-eslint": "^8.35.1"
